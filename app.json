{
  "expo": {
    "name": "ExcelContactImporter",
    "slug": "excel-contact-importer",
    "version": "1.0.0",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/84d8a972-7b83-491b-b40b-768e9cf4bfb6"
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1E40AF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.excelcontactimporter.app",
      "infoPlist": {
        "NSContactsUsageDescription": "This app needs access to your contacts to import Excel/CSV contact data into your phone book."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1E40AF"
      },
      "permissions": [
        "android.permission.READ_CONTACTS",
        "android.permission.WRITE_CONTACTS"
      ],
      "package": "com.excelcontactimporter.app"
    },
    "plugins": [
      "expo-updates",
      [
        "expo-contacts",
        {
          "contactsPermission": "Allow ExcelContactImporter to access your contacts to detect duplicates and import new contacts."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "84d8a972-7b83-491b-b40b-768e9cf4bfb6"
      }
    },
    "owner": "mganeshgani"
  }
}
